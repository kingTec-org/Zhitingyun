<?xml version="1.0" encoding="utf-8"?><!--<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"--><!--xmlns:app="http://schemas.android.com/apk/res-auto"--><!--android:layout_width="match_parent"--><!--android:layout_height="match_parent"--><!--android:background="@color/colorGrayf7"--><!--android:paddingBottom="30dp">-->

<com.qmuiteam.qmui.layout.QMUILinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/linearlayout"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/qmui_config_color_white"
    android:orientation="vertical"
    android:paddingBottom="15dp"
    app:qmui_borderColor="@color/colorWhite"
    app:qmui_outerNormalColor="@color/colorWhite">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:gravity="center_horizontal">

        <TextView
            android:id="@+id/tvLeft"
            android:layout_width="83dp"
            android:layout_height="40dp"
            android:background="@color/colorPrimary"
            android:gravity="center"
            android:text="左耳"
            android:textColor="@color/colorWhite"
            android:textSize="16sp" />

        <TextView
            android:id="@+id/tvRight"
            android:layout_width="83dp"
            android:layout_height="40dp"
            android:background="@drawable/shape_blue_button"
            android:gravity="center"
            android:text="右耳"
            android:textColor="@color/colorPrimary"
            android:textSize="16sp" />
    </LinearLayout>

    <TextView
        style="@style/text16"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="20dp"
        android:text="使用方式：小中大声"
        android:textColor="@color/colorGray33" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp">

        <TextView
            style="@style/text16"
            android:text="         压缩："
            android:textColor="@color/colorGray99" />

        <TextView
            android:id="@+id/tvStatus0"
            style="@style/text16"
            android:textColor="@color/colorGray33" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_weight="1" />

        <TextView
            style="@style/text16"
            android:text="扩展："
            android:textColor="@color/colorGray99" />

        <TextView
            android:id="@+id/tvStatus1"
            style="@style/text16"
            android:textColor="@color/colorGray33" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp"
        android:visibility="gone">

        <TextView
            style="@style/text16"
            android:text="         降噪："
            android:textColor="@color/colorGray99" />

        <TextView
            android:id="@+id/tvStatus2"
            style="@style/text16"
            android:textColor="@color/colorGray33" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_weight="1" />

        <TextView
            style="@style/text16"
            android:text="降噪等级："
            android:textColor="@color/colorGray99" />

        <TextView
            android:id="@+id/tvStatus3"
            style="@style/text16"
            android:textColor="@color/colorGray33" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="12dp"
        android:paddingLeft="12dp"
        android:paddingRight="12dp">

        <TextView
            style="@style/text16"
            android:text="降噪等级："
            android:textColor="@color/colorGray99" />

        <TextView
            android:id="@+id/tvStatus4"
            style="@style/text16"
            android:textColor="@color/colorGray33" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_weight="1" />

        <TextView
            style="@style/text16"
            android:text="反馈抑制等级："
            android:textColor="@color/colorGray99" />

        <TextView
            android:id="@+id/tvStatus5"
            style="@style/text16"
            android:textColor="@color/colorGray33" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center_vertical"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">

        <TextView
            style="@style/text16"
            android:text="音量"
            android:textColor="@color/colorGray33" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/tvVolume"
            style="@style/text16"
            android:textColor="@color/colorGray33" />
    </LinearLayout>

    <ProgressBar
        android:id="@+id/progressBar0"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layout_marginLeft="11dp"
        android:layout_marginRight="11dp"
        android:layout_marginTop="5dp"
        android:max="100"
        android:progress="0"
        android:progressDrawable="@drawable/layer_progressbar" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:gravity="center_vertical"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">

        <TextView
            style="@style/text16"
            android:text="高频"
            android:textColor="@color/colorGray33" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/tvHigh"
            style="@style/text16"
            android:textColor="@color/colorGray33" />
    </LinearLayout>

    <ProgressBar
        android:id="@+id/progressBar1"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layout_marginLeft="11dp"
        android:layout_marginRight="11dp"
        android:layout_marginTop="5dp"
        android:max="100"
        android:progress="0"
        android:progressDrawable="@drawable/layer_progressbar" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:gravity="center_vertical"
        android:paddingLeft="10dp"
        android:paddingRight="10dp"
        android:visibility="gone">

        <TextView
            style="@style/text16"
            android:text="中频"
            android:textColor="@color/colorGray33" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/tvMiddle"
            style="@style/text16"
            android:textColor="@color/colorGray33" />
    </LinearLayout>

    <ProgressBar
        android:id="@+id/progressBar2"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layout_marginLeft="11dp"
        android:layout_marginRight="11dp"
        android:layout_marginTop="5dp"
        android:max="100"
        android:progress="0"
        android:progressDrawable="@drawable/layer_progressbar"
        android:visibility="gone" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:gravity="center_vertical"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">

        <TextView
            style="@style/text16"
            android:text="低频"
            android:textColor="@color/colorGray33" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_weight="1" />

        <TextView
            android:id="@+id/tvLow"
            style="@style/text16"
            android:textColor="@color/colorGray33" />
    </LinearLayout>

    <ProgressBar
        android:id="@+id/progressBar3"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:layout_marginLeft="11dp"
        android:layout_marginRight="11dp"
        android:layout_marginTop="5dp"
        android:max="100"
        android:progress="0"
        android:progressDrawable="@drawable/layer_progressbar" />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginTop="20dp"
        android:background="@color/colorGrayf0" />

    <LinearLayout
        android:id="@+id/linearlayout11"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal"></LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorGrayf0" />

    <LinearLayout
        android:id="@+id/linearlayout12"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal"></LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorGrayf0" />

    <LinearLayout
        android:id="@+id/linearlayout13"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal"></LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorGrayf0" />

    <LinearLayout
        android:id="@+id/linearlayout14"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal"></LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorGrayf0" />

    <LinearLayout
        android:id="@+id/linearlayout15"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal"></LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorGrayf0" />

    <LinearLayout
        android:id="@+id/linearlayout16"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal"></LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorGrayf0" />

    <LinearLayout
        android:id="@+id/linearlayout17"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:orientation="horizontal"></LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:background="@color/colorGrayf0" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:gravity="center_vertical"
        android:paddingLeft="10dp"
        android:paddingRight="10dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="小中大声增益图"
            android:textColor="@color/colorGray33"
            android:textSize="14sp" />

        <View
            android:layout_width="0dp"
            android:layout_height="1dp"
            android:layout_weight="1" />

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center_vertical">

                <View
                    android:layout_width="15dp"
                    android:layout_height="2dp"
                    android:background="@color/colorPrimary" />

                <TextView
                    style="@style/text12"
                    android:layout_marginLeft="9dp"
                    android:text="中声增益" />

                <View
                    android:layout_width="15dp"
                    android:layout_height="2dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/colorGreen57" />

                <TextView
                    style="@style/text12"
                    android:layout_marginLeft="9dp"
                    android:text="大声增益" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:gravity="center_vertical">

                <View
                    android:layout_width="15dp"
                    android:layout_height="2dp"
                    android:background="@color/colorRedd0" />

                <TextView
                    style="@style/text12"
                    android:layout_marginLeft="9dp"
                    android:text="小声增益" />

                <View
                    android:layout_width="15dp"
                    android:layout_height="2dp"
                    android:layout_marginLeft="30dp"
                    android:background="@color/colorOrangeff" />

                <TextView
                    style="@style/text12"
                    android:layout_marginLeft="9dp"
                    android:text="MPO" />
            </LinearLayout>

        </LinearLayout>
    </LinearLayout>

    <com.github.mikephil.charting.charts.LineChart
        android:id="@+id/chart"
        android:layout_width="match_parent"
        android:layout_height="206dp"
        android:background="@color/colorWhite" />


</com.qmuiteam.qmui.layout.QMUILinearLayout>


    <!--</ScrollView>-->
